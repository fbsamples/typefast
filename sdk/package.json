{
  "name": "fb-typefast-sdk",
  "version": "1.0.0",
  "description": "TypeFast SDK is the barebone user-space abstraction available within TypeFast workers and it's responsible to provide an abstraction around the Facebook Graph API.",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose --coverage --no-cache",
    "transpile": "babel src/ -d build/",
    "start": "babel -w src/ -d build/"
  },
  "author": {
    "name": "Luca Bruno",
    "url": "https://github.com/pruno"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/facebook/typefast.git"
  },
  "bugs": {
    "url": "https://github.com/facebook/typefast/issues"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "immutable"
    ]
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "form-urlencoded": "^1.3.0",
    "immutable": "3.8.*",
    "sync-request": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-jest": "^12.0.2",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-polyfill": "^6.8.0",
    "jest-cli": "^12.0.2"
  }
}
