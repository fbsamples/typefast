{
  "debug": false,
  "graph": {
    "version": [0, 0],
    "application_id": 0,
    "application_secret": "APPLICATION_SECRET",
    "access_token": "ACCESS_TOKEN",
    "business_manager_id": "BUSINESS_MANAGER_ID",
    "schema": {
      "bundle": null
    }
  },
  "server": {
    "client": {
      "enable_delivery": true,
      "root": "../client/src"
    },
    "bindings": {
      "https": {
        "is_enabled": true,
        "driver": "https",
        "addr": "127.0.0.1",
        "port": 8080,
        "max_connections": 100,
        "ssl": {
          "key": "ssl/key",
          "cert": "ssl/crt"
        }
      },
      "http": {
        "is_enabled": false,
        "driver": "http",
        "addr": "127.0.0.1",
        "port": 80,
        "max_connections": 100
      },
      "http-v6": {
        "is_enabled": false,
        "driver": "http",
        "addr": "::1",
        "port": 88,
        "max_connections": 100
      }
    }
  },
  "db": {
    "url": "mongodb://localhost/typefast"
  },
  "sandbox": {
    "timeout": 86400000,
    "routine_sync_interval": 3000
  },
  "scheduler": {
    "queues": {
      "main": {
        "polling_interval": 60000
      },
      "preview": {
        "polling_interval": 1000
      }
    }
  }
}
